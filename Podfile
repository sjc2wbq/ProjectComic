# Uncomment this line to define a global platform for your project
# platform :ios, '8.0'
# Uncomment this line if you're using Swift
# use_frameworks!

inhibit_all_warnings!

target 'BaseProject' do
    pod 'AFNetworking'
    pod 'MJExtension'
    pod 'MJRefresh'
    pod 'Masonry'
    pod 'BlocksKit'
    pod 'WMPageController'#头部多个controller标题栏
    pod 'CocoaLumberjack'#优秀的替换nslog的第三方框架，更加省资源，并且可以定义打印级别
    pod 'MBProgressHUD'#实现安卓toast风格提示，弹出提示框架：如旋转的“菊花”
    pod 'TPKeyboardAvoiding'#当键盘弹出时自动把输入框textfield顶到键盘的上方
    pod 'MLTransition'#
    pod 'FMDB'#soqlite的oc封装
    pod 'CocoaAsyncSocket'#Socket通信
    pod 'RESideMenu'#侧边栏
    pod 'MWPhotoBrowser'
    pod 'iCarousel' # 3D多种效果展示库,如轮播等
    pod 'PSCollectionView' #瀑布流
    pod 'pop' #An extensible iOS and OS X animation library, useful for physics-based interactions.
    pod 'YYKit'
    pod 'FlatUIKit'#16进制转2进制（比如美工给了16进制的颜色，而oc只识别二进制，用此类库转为2进制）
    pod 'BmobSDK'
    
end

